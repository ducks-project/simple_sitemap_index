{
    "name": "drupal/simple_sitemap_index",
    "description": "Provide a Sitemap Index Variant for Simple XML sitemap.",
    "type": "drupal-module",
    "keywords": ["Drupal", "module", "sitemap", "xml", "index", "simple", "seo", "search"],
    "homepage": "https://github.com/ducks-project/simple_sitemap_index",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Adrien Loyant",
            "email": "donaldinou@gmail.com",
            "homepage": "https://github.com/donaldinou",
            "role": "Developper"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.2",
        "drupal/core": "^8.7.7 || ^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.6",
        "friendsofphp/php-cs-fixer": "^1.12"
    },
    "conflict": {
        "drupal/drupal": "<8.7.7"
    },
    "support": {
        "email": "donaldinou@gmail.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\simple_sitemap_index\\": "src"
        },
        "exclude-from-classmap": [
            "/src/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Drupal\\simple_sitemap_index\\": "src/Tests"
        }
    }
}
